{
  "name": "boilerplate-react-ts",
  "version": "1.0.0",
  "description": "boilerplate for react ts",
  "main": "index.js",
  "scripts": {
    "lint": "tslint -t stylish --project \"tsconfig.json\"",
    "clean": "rm -rf dist/server",
    "build": "npm run web:build && npm run server:build",
    "start": "npm run build && node dist/server/index.js",
    "web:build": "npm run lint && npx webpack --mode production",
    "server:build": "npm run clean && npm run lint && tsc -p tsconfig.server.json",
    "web:start": "npm run lint && webpack-dev-server --open --mode development",
    "server:start": "npm run lint && ts-node-dev --respawn --transpileOnly ./server/index.ts"
  },
  "keywords": [],
  "author": "Emman Nipal",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.9.23",
    "@types/react-dom": "^16.9.5",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "git-repo-info": "^2.1.1",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "redux-devtools": "^3.5.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "ts-loader": "^6.2.1",
    "ts-node-dev": "^1.0.0-pre.44",
    "tslint": "^6.1.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "@types/express": "^4.17.3",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-promise-middleware": "^6.0.0",
    "@types/redux-thunk": "^2.1.0",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "http-status": "^1.4.2",
    "moment-timezone": "^0.5.28",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-promise-middleware": "^6.1.2",
    "redux-thunk": "^2.3.0"
  }
}
